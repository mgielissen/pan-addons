<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
           
           <record model="ir.ui.view" id="product_product_form_view_procurment">
                <field name="name">product.product.form.view.procurment</field>
                <field name="model">product.product</field>
                <field name="inherit_id" ref="product.product_normal_form_view" />
                <field name="arch" type="xml">
                    <xpath expr="//notebook/page[@string='Procurements']" position="attributes">
                        <attribute name="groups">panatta_ext.group_procurements_tab</attribute>
                    </xpath>
                </field>
            </record>
           <record model="ir.ui.view" id="product_template_form_view_procurment">
                <field name="name">product.template.form.view.procurment</field>
                <field name="model">product.template</field>
                <field name="inherit_id" ref="product.product_template_form_view" />
                <field name="arch" type="xml">
                    <xpath expr="//notebook/page[@string='Procurements']" position="attributes">
                        <attribute name="groups">panatta_ext.group_procurements_tab</attribute>
                    </xpath>
                </field>
            </record>
            
                
    </data>
</openerp>